  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "0.1.14",
    "@mapbox/polyline": "^1.1.1",
    "@turf/turf": "^6.3.0",
    "auto-link": "^1.0.0",
    "babel-runtime": "^6.26.0",
    "classnames": "^2.2.6",
    "cors": "^2.8.5",
    "isomorphic-unfetch": "^3.1.0",
    "lodash": "^4.17.21",
    "next": "^10.0.9",
    "osrm-text-instructions": "^0.14.1-beta.3",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-ga": "^3.3.0",
    "react-modal": "^3.12.1",
    "react-no-ssr": "^1.1.0",
    "recharts": "^2.0.8"
  },
  "xo": {
    "esnext": true,
    "space": true,
    "rules": {
      "camelcase": [
        "error",
        {
          "properties": "never"
        }
      ],
      "object-curly-spacing": [
        "error",
        "always"
      ],
      "no-unused-vars": [
        "error",
        {
          "varsIgnorePattern": "^[A-Z]"
        }
      ],
      "semi": [
        "error",
        "never"
      ]
    }
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint-config-xo-nextjs": "^2.0.2",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "xo": "^0.38.2"
  }
}
